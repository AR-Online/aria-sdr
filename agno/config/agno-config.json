{
  "agno_config": {
    "bot_id": "aria-sdr-bot",
    "name": "ARIA-SDR",
    "description": "Agente de Relacionamento Inteligente da AR Online",
    "language": "pt-BR",
    "timezone": "America/Sao_Paulo",
    "webhook_url": "https://agno.ar-infra.com.br/webhook/assist/routing",
    "api_base_url": "https://agno.ar-infra.com.br/api/v1",
    "auth_token": "",
    "features": {
      "whatsapp": true,
      "web_chat": true,
      "voice": false,
      "file_upload": true
    },
    "routing": {
      "default_route": "faq",
      "volume_threshold": 1200,
      "high_volume_action": "schedule",
      "low_volume_action": "buy_credits"
    },
    "integrations": {
      "fastapi": {
        "base_url": "http://localhost:8000",
        "endpoints": {
          "routing": "/assist/routing",
          "faq": "/assist/faq",
          "health": "/healthz"
        }
      },
      "openai": {
        "assistant_id": "",
        "timeout_seconds": 12
      },
      "supabase": {
        "enabled": true,
        "rag_endpoint": "/rag/query"
      }
    }
  }
}
