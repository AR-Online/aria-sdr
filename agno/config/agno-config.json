{
  "agno_config": {
    "bot_id": "aria-sdr-bot",
    "name": "ARIA-SDR",
    "description": "Agente de Relacionamento Inteligente da AR Online",
    "language": "pt-BR",
    "timezone": "America/Sao_Paulo",
    "webhook_url": "https://agno.ar-infra.com.br/webhook/assist/routing",
    "api_base_url": "https://agno.ar-infra.com.br/api/v1",
    "auth_token": "${AGNO_AUTH_TOKEN}",
    "features": {
      "whatsapp": true,
      "web_chat": true,
      "voice": false,
      "file_upload": true
    },
    "routing": {
      "default_route": "faq",
      "volume_threshold": "${VOLUME_ALTO_LIMIAR}",
      "high_volume_action": "schedule",
      "low_volume_action": "buy_credits"
    },
    "integrations": {
      "fastapi": {
        "base_url": "http://localhost:8000",
        "auth_token": "${FASTAPI_BEARER_TOKEN}",
        "endpoints": {
          "routing": "/assist/routing",
          "faq": "/assist/faq",
          "health": "/healthz"
        }
      },
      "openai": {
        "api_key": "${OPENAI_API_KEY}",
        "assistant_id": "${ASSISTANT_ID}",
        "timeout_seconds": "${ASSISTANT_TIMEOUT_SECONDS}"
      },
      "supabase": {
        "enabled": "${RAG_ENABLE}",
        "url": "${SUPABASE_URL}",
        "service_role_key": "${SUPABASE_SERVICE_ROLE_KEY}",
        "embedding_model": "${EMBEDDING_MODEL}",
        "embedding_dim": "${EMBEDDING_DIM}",
        "rag_endpoint": "${RAG_ENDPOINT}",
        "default_source": "${RAG_DEFAULT_SOURCE}"
      }
    },
    "environment_variables": {
      "API_HOST": "${API_HOST}",
      "API_PORT": "${API_PORT}",
      "API_LOG_LEVEL": "${API_LOG_LEVEL}",
      "BEARER_TOKEN": "${BEARER_TOKEN}"
    },
    "cloudflare": {
      "api_token": "${CLOUDFLARE_API_TOKEN}",
      "features": {
        "dns": true,
        "security": true,
        "analytics": true
      }
    },
    "mindchat": {
      "api_token": "${MINDCHAT_API_TOKEN}",
      "api_base_url": "${MINDCHAT_API_BASE_URL}",
      "api_docs": "${MINDCHAT_API_DOCS}",
      "features": {
        "customer_service": true,
        "ticket_management": true,
        "chat_integration": true,
        "analytics": true
      },
      "endpoints": {
        "tickets": "/api/tickets",
        "messages": "/api/messages",
        "customers": "/api/customers",
        "analytics": "/api/analytics"
      }
    }
  }
}
